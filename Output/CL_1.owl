        <rdfs:label>present_in_taxon</rdfs:label>
        <rdfs:label xml:lang="en">present in taxon</rdfs:label>
        <oboInOwl:is_class_level rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</oboInOwl:is_class_level>
